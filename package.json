{
  "name": "sunset-sunrise",
  "displayName": "sunset-sunrise",
  "description": "",
  "version": "0.0.1",
  "publisher": "eduardn",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "properties": {
        "chameleon.lightTheme": {
          "type": "string",
          "description": " Light theme to be used during the day"
        },
        "chameleon.darkTheme": {
          "type": "string",
          "description": "Dark theme to be used during the night"
        },
        "chameleon.time": {
          "type": "string",
          "description": "Set this to 'auto' to automatically calculate the day/night hours baesd on the provided latitude and longitude"
        },
        "chameleon.day": {
          "type": ["number", "string"],
          "default": 8,
          "description": "A number representing the start of the day (24h format)"
        },
        "chameleon.night": {
          "type": ["number", "string"],
          "default": 20,
          "description": "A number representing the start of the night (24h format)"
        },
        "chameleon.latitude": {
          "type": "number",
          "description": "Latitude of your position"
        },
        "chameleon.longitude": {
          "type": "number",
          "description": "Longitude of your position"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "eslint": "^4.6.1",
    "@types/node": "^7.0.0",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "suncalc": "^1.8.0"
  }
}
