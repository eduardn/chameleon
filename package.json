{
  "name": "sunset-sunrise",
  "displayName": "sunset-sunrise",
  "description": "",
  "version": "0.0.1",
  "publisher": "eduardn",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "properties": {
        "chameleon.lightTheme": {
          "type": "string",
          "description": "Theme to be used during daytime"
        },
        "chameleon.darkTheme": {
          "type": "string",
          "description": "Theme to be used during night"
        },
        "chameleon.time": {
          "type": "string",
          "default": "auto"
        },
        "chameleon.time.day": {
          "type": "number",
          "default": 8
        },
        "chameleon.time.night": {
          "type": "number",
          "default": 20
        },
        "chameleon.latitude": {
          "type": "number"
        },
        "chameleon.longitude": {
          "type": "number"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "eslint": "^4.6.1",
    "@types/node": "^7.0.0",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "suncalc": "^1.8.0"
  }
}
